{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        {
            "src": "/users/all",
            "dest": "/server.js"
        },
        {
            "src": "/users/:name",
            "dest": "/server.js"
        },
        {
            "src": "/users",
            "dest": "/server.js"
        },
        {
            "src": "/scrape",
            "dest": "/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}
